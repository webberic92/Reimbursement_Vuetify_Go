{
  "name": "vuetify-nav",
  "version": "1.2.1",
  "description": "Navigation menu components for vuetify",
  "author": "Dr. Takeyuki UEDA",
  "license": "MIT",
  "keywords": [
    "vue",
    "vuetify",
    "toolbar",
    "navigation drawer",
    "menu",
    "qr code"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/UedaTakeyuki/vuetify-nav.git"
  },
  "private": false,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-bundle": "vue-cli-service build --target lib --name vuetify-nav --formats umd ./src/build-entry.js",
    "build-serve": "npx serve -s dist",
    "lint": "vue-cli-service lint"
  },
  "main": "./src/build-entry.js",
  "files": [
    "src/components/Navbar.vue",
    "src/build-entry.js"
  ],
  "dependencies": {
    "qrcode-of-this-site": "^1.0.5"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.10.0",
    "@vue/cli-plugin-eslint": "^3.10.0",
    "@vue/cli-service": "^3.10.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "sass": "^1.17.4",
    "sass-loader": "^7.1.0",
    "vue-cli-plugin-vuetify": "^0.6.3",
    "vue-template-compiler": "^2.6.10",
    "vuetify-loader": "^1.2.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "stylelint": "refer https://qiita.com/saio-th/items/111f6f5cc62f421cf045",
      "no-console": 0
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
