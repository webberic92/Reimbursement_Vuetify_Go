,EAAA;AACA,IAAA,MADA,wBACA;AAAA;;AAAA,UAAA,MAAA,QAAA,MAAA;AACA,WAAA,KAAA,CAAA,QAAA,CAAA,QAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,mBAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,IAAA,CAAA,SAAA,CAAA,KAAA,IAAA,CAAA;AACA,MAAA,KAAA,CAAA;AACA,QAAA,MAAA,EAAA,MADA;AAEA,QAAA,GAAA,EAAA,iCAFA;AAGA,QAAA,IAAA,EAAA,KAAA;AAHA,OAAA,CAAA,CAKA,IALA;AAAA,4EAKA,iBAAA,QAAA;AAAA;AAAA;AAAA;AAAA;AACA,sBAAA,CAAA,QAAA,CAAA,EAAA,EAAA;AACA,oBAAA,OAAA,CAAA,GAAA,CAAA,QAAA,CAAA,IAAA,CAAA,OAAA;AACA,oBAAA,KAAA,CAAA,YAAA,GAAA,EAAA;AACA,oBAAA,KAAA,CAAA,cAAA,GAAA,QAAA,CAAA,IAAA,CAAA,OAAA,CAHA,CAIA;AACA;AACA;AACA;AACA;;AACA,oBAAA,MAAA,CAAA,cAAA,CAAA;AAGA,oBAAA,KAAA,CAAA,cAAA,GAAA,+BAAA;AACA,oBAAA,UAAA,CAAA,YAAA;AACA,sBAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA,OAAA;AACA,qBAFA,EAEA,IAFA,CAAA;AAGA;;AAjBA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,SALA;;AAAA;AAAA;AAAA;AAAA,WAwBA,KAxBA,CAwBA,UAAA,KAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA,CAAA,QAAA;AACA,QAAA,KAAA,CAAA,YAAA,GAAA,KAAA,CAAA,QAAA;AACA,QAAA,KAAA,CAAA,cAAA,GAAA,EAAA,CAHA,CAIA;AAEA,OA9BA;AA+BA,KApCA;AAqCA,IAAA,KArCA,mBAqCA;AACA,WAAA,KAAA,GAAA,EAAA;AACA,WAAA,QAAA,GAAA,EAAA;AACA,WAAA,KAAA,CAAA,QAAA,CAAA,KAAA;AACA,WAAA,YAAA,GAAA,EAAA;AACA,WAAA,cAAA,GAAA,EAAA;AACA,WAAA,cAAA,GAAA,EAAA;AACA;AA5CA;AAlBA,CAAA","sourcesContent":["<template>\r\n  <v-container class=\"grey lighten-5\" fill-height fluid>\r\n    <v-card class=\"pa-md-4 mx-lg-auto\" color=\"white\" width=\"600px\">\r\n     \r\n      <!-- Messages -->\r\n      <p v-if=\"errorMessage\" align=\"center\" style=\"color:red\">\r\n        <strong>\r\n          {{ errorMessage }}\r\n        </strong>\r\n      </p>\r\n      <p v-if=\"successMessage\" align=\"center\" style=\"color:green\">\r\n        <strong>{{ successMessage }}</strong>\r\n      </p>\r\n\r\n      <p v-if=\"routingMessage\" align=\"center\">\r\n        <strong> {{ routingMessage }} </strong>\r\n      </p>\r\n      <v-row>\r\n        <v-col cols=\"12\" sm=\"4\"> </v-col>\r\n      </v-row>\r\n\r\n      <validation-observer ref=\"observer\" v-slot=\"{ invalid }\">\r\n        <form @submit.prevent=\"submit\">\r\n\r\n\r\n          <!-- Email -->\r\n\r\n          <validation-provider\r\n            v-slot=\"{ errors }\"\r\n            name=\"email\"\r\n            rules=\"required|email\"\r\n          >\r\n            <v-text-field\r\n              v-model=\"form.email\"\r\n              :error-messages=\"errors\"\r\n              label=\"E-mail\"\r\n              required\r\n            ></v-text-field>\r\n          </validation-provider>\r\n\r\n          <!-- Password -->\r\n\r\n          <validation-provider\r\n            v-slot=\"{ errors }\"\r\n            name=\"password\"\r\n            rules=\"required\"\r\n          >\r\n            <v-text-field\r\n              type=\"password\"\r\n              v-model=\"form.password\"\r\n              :error-messages=\"errors\"\r\n              label=\"Password\"\r\n              required\r\n            ></v-text-field>\r\n          </validation-provider>\r\n\r\n       \r\n          <v-btn class=\"mr-4\" type=\"submit\" :disabled=\"invalid\">\r\n            submit\r\n          </v-btn>\r\n          <v-btn @click=\"clear\">\r\n            clear\r\n          </v-btn>\r\n        </form>\r\n      </validation-observer>\r\n    </v-card>\r\n  </v-container>\r\n</template>\r\n\r\n<script>\r\nimport { required, email } from \"vee-validate/dist/rules\";\r\nimport {\r\n  extend,\r\n  ValidationObserver,\r\n  ValidationProvider,\r\n  setInteractionMode,\r\n} from \"vee-validate\";\r\nimport axios from \"axios\";\r\n\r\nsetInteractionMode(\"eager\");\r\n\r\nextend(\"required\", {\r\n  ...required,\r\n  message: \"{_field_} can not be empty\",\r\n});\r\n\r\nextend(\"email\", {\r\n  ...email,\r\n  message: \"Email must be valid\",\r\n});\r\n\r\n\r\nexport default {\r\n  components: {\r\n    ValidationProvider,\r\n    ValidationObserver,\r\n  },\r\n  data() {\r\n    return {\r\n      errorMessage: \"\",\r\n      successMessage: \"\",\r\n      routingMessage: \"\",\r\n      form: {\r\n        email: \"\",\r\n        password: \"\",\r\n      },\r\n\r\n      typeOfUser: [\"Regular User\", \"Administrator User\"],\r\n    };\r\n  },\r\n  methods: {\r\n    submit({commit}) {\r\n      this.$refs.observer.validate();\r\n      console.log(\"submit button hit\");\r\n      console.log(JSON.stringify(this.form));\r\n      axios({\r\n        method: \"post\",\r\n        url: \"http://localhost:8000/api/login\",\r\n        data: this.form,\r\n      })\r\n        .then(async (response) => {\r\n          if (!response.ok) {\r\n            console.log(response.data.message);\r\n            this.errorMessage = \"\";\r\n            this.successMessage = response.data.message;\r\n        // const token = response.data.token\r\n        // localStorage.setItem('token', token)\r\n        // console.log(token)\r\n        //  axios.defaults.headers.common['Authorization'] = token\r\n        // commit('auth_success')\r\n          commit('auth_success')\r\n\r\n\r\n            this.routingMessage = \"Now routing you to login page\";\r\n            setTimeout(() => {\r\n              this.$router.push(\"/home\");\r\n            }, 3000);\r\n          }\r\n        })\r\n        .catch((error) => {\r\n          console.log(error.response);\r\n          this.errorMessage = error.response;\r\n          this.successMessage - \"\";\r\n          //localStorage.removeItem('token')\r\n\r\n        });\r\n    },\r\n    clear() {\r\n      this.email = \"\";\r\n      this.password = \"\";\r\n      this.$refs.observer.reset();\r\n      this.errorMessage = \"\";\r\n      this.successMessage = \"\";\r\n      this.routingMessage = \"\";\r\n    },\r\n  },\r\n};\r\n</script>\r\n"],"sourceRoot":"src/views"}]}