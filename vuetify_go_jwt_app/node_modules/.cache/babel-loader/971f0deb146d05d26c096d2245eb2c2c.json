AAA;AAKA,MAAA,CAAA,KAAA,kCACA,GADA;AAEA,EAAA,OAAA,EAAA;AAFA,GAAA;AAKA,MAAA,CAAA,OAAA,kCACA,KADA;AAEA,EAAA,OAAA,EAAA;AAFA,GAAA;AAKA,MAAA,CAAA,OAAA,kCACA,KADA;AAEA,EAAA,OAAA,EAAA;AAFA,GAAA;AAOA,eAAA;AACA,EAAA,UAAA,EAAA;AACA,IAAA,kBAAA,EAAA,kBADA;AAEA,IAAA,kBAAA,EAAA;AAFA,GADA;AAKA,EAAA,OALA,qBAKA;AAAA;;AACA,IAAA,KAAA,CAAA,GAAA,CAAA,gCAAA,EAAA;AACA,MAAA,eAAA,EAAA,IADA;AAEA,MAAA,OAAA,EAAA;AACA,uCAAA,GADA;AAEA,wBAAA;AAFA;AAFA,KAAA,EAOA,IAPA;AAAA,yEAOA,iBAAA,QAAA;AAAA;AAAA;AAAA;AAAA;AACA,oBAAA,CAAA,QAAA,CAAA,EAAA,EAAA;AACA,kBAAA,KAAA,CAAA,SAAA,GAAA,QAAA,CAAA,IAAA,CAAA,SAAA;AACA,kBAAA,OAAA,CAAA,GAAA,CAAA,KAAA,CAAA,MAAA,CAAA,KAAA,CAAA,UAAA;AACA,kBAAA,OAAA,CAAA,GAAA,CAAA,KAAA,CAAA,MAAA,CAAA,OAAA,CAAA,QAAA;;AACA,sBAAA,QAAA,CAAA,IAAA,CAAA,QAAA,IAAA,GAAA,EAAA;AACA,oBAAA,KAAA,CAAA,QAAA,GAAA,SAAA;AACA,mBAFA,MAEA,IAAA,QAAA,CAAA,IAAA,CAAA,QAAA,IAAA,GAAA,EAAA;AACA,oBAAA,KAAA,CAAA,QAAA,GAAA,OAAA;AACA,oBAAA,KAAA,CAAA,MAAA,CAAA,eAAA;AACA;AACA;;AAXA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,OAPA;;AAAA;AAAA;AAAA;AAAA,SAoBA,KApBA,CAoBA,UAAA,KAAA,EAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA,CAAA,QAAA;;AACA,MAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA,GAAA;AACA,KAvBA;AAwBA,GA9BA;AA+BA,EAAA,IA/BA,kBA+BA;AACA,WAAA;AACA,MAAA,IAAA,EAAA;AACA,QAAA,SAAA,EAAA;AADA;AADA,KAAA;AAKA,GArCA;AAsCA,EAAA,OAAA,EAAA;AACA,IAAA,KADA,mBACA;AACA,WAAA,SAAA,GAAA,EAAA;AACA;AAHA;AAtCA,CAAA","sourcesContent":["<template>\r\n\r\n\r\n\r\n\r\n<!-- ADMIN STUFF -->\r\n  <div v-if=\"this.$store.state.isAdmin\">ADMIN STUFF HERE\r\n\r\n <p align=\"center\">\r\n    <strong>\r\n      Welcome {{ this.firstName }}\r\n      <br />\r\n      <br />\r\n      You are currently logged in as a\r\n      {{ this.userType }} <br />\r\n    </strong>\r\n    <br />\r\n  </p>\r\n\r\n\r\n  </div>\r\n\r\n<!-- Regular User Stuff -->\r\n  <div align=\"center\" v-else-if=\"!this.$store.state.isAdmin\">REGULAR USER STUFF HERE\r\n\r\n\r\n\r\n\r\n\r\n   <v-container class=\"grey lighten-5\" fill-height fluid>\r\n\r\n\r\n\r\n\r\n\r\n    <v-card class=\"pa-md-4 mx-lg-auto\" color=\"white\" width=\"auto\">\r\n     <p>\r\n    <strong>\r\n      Welcome {{ this.firstName }}\r\n      <br />\r\n      <br /> \r\n      You are currently logged in as a\r\n      {{ this.userType }} User <br />\r\n    </strong>\r\n    <br />\r\n  </p>\r\n\r\n      <validation-observer ref=\"observer\" v-slot=\"{ invalid }\">\r\n        <form @submit.prevent=\"submit\">\r\n          <!-- First Name   -->\r\n          <validation-provider\r\n            v-slot=\"{ errors }\"\r\n            name=\"Firt Name\"\r\n            rules=\"required|max:20\"\r\n          >\r\n            <v-text-field\r\n              v-model=\"form.firstName\"\r\n              :counter=\"20\"\r\n              :error-messages=\"errors\"\r\n              label=\"First Name\"\r\n              required\r\n            ></v-text-field>\r\n          </validation-provider>\r\n\r\n\r\n          <v-btn class=\"mr-4\" type=\"submit\" :disabled=\"invalid\">\r\n            submit\r\n          </v-btn>\r\n          <v-btn @click=\"clear\">\r\n            clear\r\n          </v-btn>\r\n        </form>\r\n      </validation-observer>\r\n    </v-card>\r\n  </v-container>\r\n\r\n\r\n  </div>\r\n\r\n</template>\r\n\r\n<script>\r\nimport Axios from \"axios\";\r\nimport store from \"..//store/index\";\r\nimport { required, digits, email, max, regex } from \"vee-validate/dist/rules\";\r\nimport {\r\n  extend,\r\n  ValidationObserver,\r\n  ValidationProvider,\r\n  setInteractionMode,\r\n} from \"vee-validate\";\r\n\r\nsetInteractionMode(\"eager\");\r\n\r\nextend(\"digits\", {\r\n  ...digits,\r\n  message: \"{_field_} needs to be {length} digits. ({_value_})\",\r\n});\r\n\r\nextend(\"required\", {\r\n  ...required,\r\n  message: \"{_field_} can not be empty\",\r\n});\r\n\r\nextend(\"max\", {\r\n  ...max,\r\n  message: \"{_field_} may not be greater than {length} characters\",\r\n});\r\n\r\nextend(\"regex\", {\r\n  ...regex,\r\n  message: \"{_field_} {_value_} does not match {regex}\",\r\n});\r\n\r\nextend(\"email\", {\r\n  ...email,\r\n  message: \"Email must be valid\",\r\n});\r\n\r\n\r\n\r\nexport default {\r\n  components: {\r\n      ValidationProvider,\r\n    ValidationObserver,\r\n  },\r\n  created() {\r\n    Axios.get(\"http://localhost:8000/api/user\", {\r\n      withCredentials: true,\r\n      headers: {\r\n        \"Access-Control-Allow-Origin\": \"*\",\r\n        \"Content-Type\": \"application/json\",\r\n      },\r\n    })\r\n      .then(async (response) => {\r\n        if (!response.ok) {\r\n          this.firstName = response.data.firstName;\r\n          console.log(this.$store.state.isLoggedIn);\r\n          console.log(this.$store.getters.loggedIn);\r\n          if (response.data.userType == \"R\") {\r\n            this.userType = \"Regular\";\r\n          } else if (response.data.userType == \"A\") {\r\n            this.userType = \"Admin\";\r\n            store.commit(\"makeUserAdmin\");\r\n          }\r\n        }\r\n      })\r\n      .catch((error) => {\r\n        console.log(error.response);\r\n        this.$router.push(\"/\");\r\n      });\r\n  },\r\n  data() {\r\n    return {\r\n      form:{\r\n            firstName: \"\"\r\n      }\r\n    };\r\n  },\r\n  methods: {\r\n clear() {\r\n      this.firstName = \"\";\r\n    },\r\n\r\n\r\n  },\r\n};\r\n</script>\r\n"],"sourceRoot":"src/views"}]}