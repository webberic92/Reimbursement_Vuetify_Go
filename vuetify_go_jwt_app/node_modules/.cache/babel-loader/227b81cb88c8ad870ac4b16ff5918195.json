A,OAAA,EAAA;AACA,IAAA,MADA,oBACA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,IADA,GACA,QAAA,CAAA;AACA,kBAAA,KAAA,EAAA,EADA;AAEA,kBAAA,QAAA,EAAA;AAFA,iBAAA,CADA;;AAMA,gBAAA,OANA;AAAA,qFAMA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,mCACA,KAAA,CAAA,iCAAA,EACA;AACA,8BAAA,MAAA,EAAA,MADA;AAEA,8BAAA,OAAA,EAAA;AAAA,gDAAA;AAAA,+BAFA;AAGA,8BAAA,WAAA,EAAA,SAHA;AAIA,8BAAA,IAAA,EAAA,IAAA,CAAA,SAAA,CAAA,IAAA;AAJA,6BADA,CAAA,CAMA,IANA;AAAA,kGAMA,iBAAA,QAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA,4CACA,QAAA,CAAA,EADA;AAAA;AAAA;AAAA;;AAAA;AAAA,+CAEA,QAAA,CAAA,IAAA,EAFA;;AAAA;AAEA,wCAAA,KAFA;AAGA,wCAAA,OAAA,CAAA,GAAA,CAAA,KAAA,CAAA,OAAA;;AAHA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,+BANA;;AAAA;AAAA;AAAA;AAAA,gCADA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,mBANA;;AAAA,kCAMA,OANA;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAmBA,KApBA;AAqBA,IAAA,KArBA,mBAqBA;AACA,WAAA,KAAA,GAAA,EAAA;AACA,WAAA,QAAA,GAAA,EAAA;AACA,WAAA,KAAA,CAAA,QAAA,CAAA,KAAA;AACA,WAAA,YAAA,GAAA,EAAA;AACA,WAAA,cAAA,GAAA,EAAA;AACA,WAAA,cAAA,GAAA,EAAA;AACA;AA5BA;AAlBA,CAAA","sourcesContent":["<template>\r\n  <v-container class=\"grey lighten-5\" fill-height fluid>\r\n    <v-card class=\"pa-md-4 mx-lg-auto\" color=\"white\" width=\"600px\">\r\n     \r\n      <!-- Messages -->\r\n      <p v-if=\"errorMessage\" align=\"center\" style=\"color:red\">\r\n        <strong>\r\n          {{ errorMessage }}\r\n        </strong>\r\n      </p>\r\n      <p v-if=\"successMessage\" align=\"center\" style=\"color:green\">\r\n        <strong>{{ successMessage }}</strong>\r\n      </p>\r\n\r\n      <p v-if=\"routingMessage\" align=\"center\">\r\n        <strong> {{ routingMessage }} </strong>\r\n      </p>\r\n      <v-row>\r\n        <v-col cols=\"12\" sm=\"4\"> </v-col>\r\n      </v-row>\r\n\r\n      <validation-observer ref=\"observer\" v-slot=\"{ invalid }\">\r\n        <form @submit.prevent=\"submit\">\r\n\r\n\r\n          <!-- Email -->\r\n\r\n          <validation-provider\r\n            v-slot=\"{ errors }\"\r\n            name=\"email\"\r\n            rules=\"required|email\"\r\n          >\r\n            <v-text-field\r\n              v-model=\"form.email\"\r\n              :error-messages=\"errors\"\r\n              label=\"E-mail\"\r\n              required\r\n            ></v-text-field>\r\n          </validation-provider>\r\n\r\n          <!-- Password -->\r\n\r\n          <validation-provider\r\n            v-slot=\"{ errors }\"\r\n            name=\"password\"\r\n            rules=\"required\"\r\n          >\r\n            <v-text-field\r\n              type=\"password\"\r\n              v-model=\"form.password\"\r\n              :error-messages=\"errors\"\r\n              label=\"Password\"\r\n              required\r\n            ></v-text-field>\r\n          </validation-provider>\r\n\r\n       \r\n          <v-btn class=\"mr-4\" type=\"submit\" :disabled=\"invalid\">\r\n            submit\r\n          </v-btn>\r\n          <v-btn @click=\"clear\">\r\n            clear\r\n          </v-btn>\r\n\r\n          <v-btn @click=\"submit2\">\r\n            2\r\n          </v-btn>\r\n\r\n        </form>\r\n      </validation-observer>\r\n    </v-card>\r\n  </v-container>\r\n</template>\r\n\r\n<script>\r\nimport { required, email } from \"vee-validate/dist/rules\";\r\nimport {\r\n  extend,\r\n  ValidationObserver,\r\n  ValidationProvider,\r\n  setInteractionMode,\r\n} from \"vee-validate\";\r\nimport reactive from 'vue'\r\n\r\nsetInteractionMode(\"eager\");\r\n\r\nextend(\"required\", {\r\n  ...required,\r\n  message: \"{_field_} can not be empty\",\r\n});\r\n\r\nextend(\"email\", {\r\n  ...email,\r\n  message: \"Email must be valid\",\r\n});\r\n\r\n\r\nexport default {\r\n  components: {\r\n    ValidationProvider,\r\n    ValidationObserver,\r\n  },\r\n  data() {\r\n    return {\r\n      errorMessage: \"\",\r\n      successMessage: \"\",\r\n      routingMessage: \"\",\r\n      form: {\r\n        email: \"\",\r\n        password: \"\",\r\n      },\r\n\r\n      typeOfUser: [\"Regular User\", \"Administrator User\"],\r\n    };\r\n  },\r\n  methods: {\r\n    async submit() { \r\n  const data = reactive({\r\n    email:'',\r\n    password:''\r\n  });\r\n\r\n  const submit2 = async() => {\r\n    await fetch ('http://localhost:8000/api/login',\r\n        {\r\n          method: 'POST',\r\n          headers: { 'Content-Type': 'application/json'},\r\n          credentials:'include',\r\n          body: JSON.stringify(data)\r\n        }) .then(async response => {\r\n      if (!response.ok) {\r\n        const data = await response.json();\r\n        console.log(data.message)\r\n      }\r\n    })\r\n  }},\r\n    clear() {\r\n      this.email = \"\";\r\n      this.password = \"\";\r\n      this.$refs.observer.reset();\r\n      this.errorMessage = \"\";\r\n      this.successMessage = \"\";\r\n      this.routingMessage = \"\";\r\n    },\r\n  },\r\n};\r\n</script>\r\n"],"sourceRoot":"src/views"}]}