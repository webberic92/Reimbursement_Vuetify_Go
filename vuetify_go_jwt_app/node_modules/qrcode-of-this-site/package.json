{
  "name": "qrcode-of-this-site",
  "version": "1.1.2",
  "description": "vue component to show qrcode of this site",
  "author": {
    "name": "Dr. Takeyuki UEDA",
    "email": "ueda@latelierdueda.com",
    "url": "https://atelierueda.uedasoft.com/"
  },
  "homepage": "https://github.com/UedaTakeyuki/qrcode-of-this-site",
  "bugs": {
    "url": "https://github.com/UedaTakeyuki/qrcode-of-this-site/issues"
  },
  "license": "MIT",
  "private": false,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-bundle": "vue-cli-service build --target lib --name qrcode-of-this-site --formats umd ./src/build-entry.js",
    "lint": "vue-cli-service lint"
  },
  "main": "./src/build-entry.js",
  "files": [
    "src/components/QRcode.vue",
    "src/build-entry.js"
  ],
  "dependencies": {
    "core-js": "^2.6.5",
    "qrcode": "^1.4.4",
    "vue": "^2.6.10",
    "vue-router": "^3.0.3",
    "vuetify": "^2.0.0"
  },
  "devDependencies": {
    "qrcode": "^1.4.4"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "stylelint": "refer https://qiita.com/saio-th/items/111f6f5cc62f421cf045",
      "no-console": 0
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
